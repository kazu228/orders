<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title><%= title %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="stylesheets/style.css">
</head>

<body>
    <header>
        <h1><%= title %></h1>
    </header>
    <main>
        <table>
            <% content.forEach(function(obj,key){ %>
                <tr>
                    <th><%= obj.id %></th>
                    <td><%= obj.name %></td>
                    <td><%= obj.mail %></td>
                    <td><%= obj.age %></td>
                </tr>
            <% }); %>
        </table>
    </main>
</body>

</html>